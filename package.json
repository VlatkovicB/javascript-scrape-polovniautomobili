{
  "name": "scrape-polovniautomibili",
  "version": "1.0.0",
  "description": "Small demo of puppeteer.",
  "main": "scrape.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/server.js",
    "cron": "node -r dotenv/config index.js",
    "cron:dev": "nodemon --inspect=0.0.0.0:5003 -r dotenv/config index.js",
    "start:dev": "concurrently --kill-others \"yarn cron:dev\" \"yarn debug\"",
    "debug": "nodemon --inspect=0.0.0.0:5002 -r dotenv/config src/server.js"
  },
