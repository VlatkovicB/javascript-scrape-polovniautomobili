    "express-validator": "^6.14.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^10.0.0",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "2.0.15"
  }
